<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gqq.rxjavahelper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rxjava1.Main1Activity" />
        <activity android:name=".rxjava2.Main2Activity" />
        <activity android:name=".rxjava3.Main3Activity" />
        <activity android:name=".rxjava4.Main4Activity" />
        <activity android:name=".rxjava5.Main5Activity" />
        <activity android:name=".rxjava6.Main6Activity"></activity>
    </application>

</manifest>